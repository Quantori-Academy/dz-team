{
    "name": "be",
    "version": "0.0.0",
    "prisma": {
        "seed": "node prisma/seed.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
        "dev:db": "yarn db && run-p dev studio",
        "db:build": "docker-compose build",
        "db": "docker-compose up -d & yarn migrate:dev",
        "db:seed": "prisma db seed",
        "db:stop": "docker-compose down",
        "studio": "prisma studio",
        "migrate:dev": "prisma migrate dev",
        "preview": "yarn build && cross-env NODE_ENV=development node dist/index.js",
        "prod": "prisma generate && prisma migrate deploy && cross-env NODE_ENV=production node dist/index.js"
    },
    "dependencies": {
        "@fastify/cors": "^9.0.1",
        "@fastify/swagger": "^7.6.1",
        "@prisma/client": "^5.20.0",
        "fastify": "^4.28.1",
        "fastify-type-provider-zod": "^4.0.2",
        "js-yaml": "^4.1.0",
        "prisma": "^5.20.0",
        "shared": "0.0.0"
    },
    "devDependencies": {
        "@types/node": "^22.5.2",
        "cross-env": "^7.0.3",
        "tsx": "^4.19.1",
        "typescript": "^5.5.4",
        "zod-prisma-types": "^3.1.8"
    }
}
