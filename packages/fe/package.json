{
	"name": "fe",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && cross-env NODE_ENV=production vite build",
		"build:dev": "tsc -b && cross-env NODE_ENV=development vite build --mode development",
		"preview": "yarn build:dev && vite preview",
		"prod": "yarn build && vite preview",
		"lint:types": "tsc --noEmit",
		"lint:eslint": "eslint --ext .ts,tsx,js --ignore-path .gitignore .",
		"lint:fix": "yarn run lint:eslint -- --fix",
		"format:check": "prettier --check \"**/*.{ts,tsx,json}\"",
		"format": "prettier --write \"**/*.{ts,tsx,json}\""
	},
	"dependencies": {
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@mui/icons-material": "^6.0.2",
		"@mui/material": "^6.0.2",
		"clsx": "^2.1.1",
		"dayjs": "^1.11.13",
		"effector": "^23.2.2",
		"effector-react": "^23.2.1",
		"immer": "^10.1.1",
		"lodash": "^4.17.21",
		"patronum": "^2.2.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@types/lodash": "^4.17.7",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^8.4.0",
		"@typescript-eslint/parser": "^8.4.0",
		"@vitejs/plugin-react": "^4.3.1",
		"cross-env": "^7.0.3",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-effector": "^0.14.0",
		"eslint-plugin-react": "^7.35.2",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"typescript": "^5.5.3",
		"vite": "^5.4.1",
		"vite-plugin-svgr": "^4.2.0",
		"vite-tsconfig-paths": "^5.0.1"
	}
}
